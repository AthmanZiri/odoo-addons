<?xml version="1.0"?>
<openerp>
    <data noupdate="1">

        <!-- Scheduler job to purge old data -->
        <record id="ir_cron_import_loader_purge" model="ir.cron">
            <field name="name">Purge Import Loader old data</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
            <field eval="'import.loader'" name="model"/>
            <field eval="'do_purge'" name="function"/>
            <field eval="'()'" name="args"/>
        </record>

    </data>
</openerp>
